--map

function draw_map()
	map(0,0,0,0,128,64)
end

function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

--pick up hot dog

compteurh=0

function spawn_hotdogs()

	while compteurh!=8 do
		
		hotdogx = flr(rnd(30))
		hotdogy = flr(rnd(15))


		new_hotdog={
			x=hotdogx*8,
			y=hotdogy*8
		}
	
--	create_message("x="..tostr(hotdogx))
	
		if (check_flag(3,hotdogx,hotdogy)
					and hotdogx!=7 
					and hotdogy!=8)
		then
		mset(hotdogx,hotdogy,34)
		compteurh+=1
		end
	end
end

function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end


function pick_hotdog(x,y)
	next_tile(x,y)
	p.sprite=32
	p.hotdogs+=1
	sfx(0)
end
