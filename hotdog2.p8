--player

function create_player()
	p={
		x=7,
		y=8,
		sprite=32,
		f=true,--add to
		hotdogs=0,
		champi=0
	}
end

function player_movement()
	newx=p.x
	newy=p.y
	if (btnp(➡️)) newx+=1 f=false
	if (btnp(⬅️)) newx-=1 f=true
	if (btnp(⬇️)) newy+=1
	if (btnp(⬆️)) newy-=1
	
	interact_hotdog(newx,newy)
	interact_champi(newx,newy)
	interact_ball(newx,newy)
	
	if not check_flag(0,newx,newy) then
		p.x=mid(0,newx,30 )
		p.y=mid(0,newy,15)
	end
end

function update_camera()
	local map_width = 30
	local map_height = 15
	camx=mid(0,(p.x-7.5)*8, (map_width-15)*8)
	camy=mid(0,(p.y-7.5)*8, (map_height-15)*8)
	camera(camx,camy)
end

function interact_hotdog(x,y)
	if check_flag(1,x,y) then
		pick_hotdog(x,y)
		create_prout(p.x*8,p.y*8)
	end
end

function draw_player()
	spr(p.sprite,p.x*8,p.y*8,1,1,f)
end

