--champi gameover

function pick_champi(x,y)
	sfx(1)
	mset(x,y,3)
end

function interact_champi(x,y)
	if check_flag(4,x,y) then
				pick_champi(x,y)
				p.sprite=33
				p.hotdogs-=1
	end
end

function spawn_champi()
	compteur_champi=0
		
		while compteur_champi!=10 do
			champix = flr(rnd(30))
			champiy = flr(rnd(30))
			
			new_champi={
				x=champix*8,
				y=champiy*8
			}
	
			if (check_flag(3,champix,champiy) 
						and champix!=7 
						and champiy!=8)
			then
			mset(champix,champiy,36)
			compteur_champi+=1
			end
		end
	end

--gameover

function loose()
	if (p.hotdogs<0) then
	state=1 
	sfx(2)
	end
end

function draw_gameover()
	cls()
	camera(0,0)
	spr(25,46,25,4,4)
	print("game over",45,60,10)
	print("press ❎ to play again"
	,20,70,7)
	text.active=false
end

function update_gameover()
 	for i=0,15 do
 		pal(i,i+128,1)
 end
	if (btn(❎)) run()
end

--win the game 

function spawn_ball()

	if (p.hotdogs==3
	and is_there_ball==false) then
	
		ballx = flr(rnd(30))
		bally = flr(rnd(30))
		new_win={
			x = ballx*8,
			y = bally*8
			}
		if (check_flag(3,ballx,bally) 
		and is_there_ball==false)
		 then
			mset(ballx,bally,49)
			is_there_ball=true
		end
	end
end


function pick_ball(x,y)
 music(03)
	mset(x,y,3)
end

function interact_ball(x,y)
	if check_flag(5,x,y) then
		pick_ball(x,y)
		state=2
	end
end

function draw_win()
	cls()
	camera(0,0)
	spr(21,46,25,4,4)
	print("you win!",48,60,10)
	print("press ❎ to play again"
	,19,70,7)
	text.active=false
end

function update_win()
	if (btn(❎)) run()
end
