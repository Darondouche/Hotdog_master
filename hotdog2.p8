--game text

function init_text()	
	text={}
	text.active=false
	text.str={}
	text.hotdog_displayed=false
	text.ball_displayed=false
end

function update_text()
	
	if (p.hotdogs==2 and text.hotdog_displayed==false) then
		new_text({"hmmm les saucisses","c'est delicieux !"})
		text.hotdog_displayed=true
	end
	if (is_there_ball==true and text.ball_displayed==false) then
		new_text({"la babaaaaaalle !"})
		text.ball_displayed=true
	end
	if (btnp(❎)) then
		text.active=false
	end

end
	

function new_text(txt)
	text.str=txt
	text.active=true
end

function draw_text()

	--box same size text
	local maxw=0
	for i=1,#text.str do
		if #text.str[i]>maxw then
			maxw=#text.str[i]
		end
	end
	
	local x=(camx+40)-maxw*2-1
	local y=(camy+110)
	local w=x+(maxw*4) --4=largeur caracteres
	local h=y+#text.str*6
	
	--draw text box
	rectfill(x-1,y-1,w+1,h+1,7) --aj 1 pour la bordure
	rectfill(x,y,w,h,2)
	
	--write text
	for i=1,#text.str do
		local txt=text.str[i]
		local tx=(camx+40)-#txt*2 --pour centrer texte
		local ty=y-5+(i*6) --hauteur texte=6px
		print(txt,tx,ty,7)
	end
end
