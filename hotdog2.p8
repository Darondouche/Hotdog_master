version 38
__lua__
function _init()
	create_player()
	prouts={}
	spawn_hotdogs()
	spawn_champi()
	init_text()
	state=3
	if state==3 then music(19)
	end
	is_there_ball=false
	camx=0
	camy=0
end

function _update()
	if (state==0) update_game() 	
	if (state==1) update_gameover()
	if (state==2) update_win()
	if (state==3) update_logo()
end

function update_game()
	player_movement()
	update_camera()
	interact_hotdog()
	interact_champi()
	interact_ball()
	loose()
	update_prouts()
	count_ball_time()
	delete_ball()
	update_text()
end

function _draw()
	if (state==0) draw_game()
	if (state==1) draw_gameover()
	if (state==2) draw_win()
	if (state==3) draw_logo()
	if text.active==true then
		draw_text()
	end
end


function draw_game()
	cls()
	music(-1)
	draw_map()
	draw_player()
	draw_prouts()
	draw_score()
	draw_message()
	spawn_ball()
end
