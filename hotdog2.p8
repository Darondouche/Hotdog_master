--prouts
function create_prout(x,y)
	sfx(0)
	add(prouts,{x=x,
	            y=y,
	            timer=0})
end

function update_prouts()
	for pr in all(prouts) do
		pr.timer+=1
		if pr.timer==13 then
			del(prouts,pr)
		end
	end
end

function draw_prouts()
	circ(x,y,rayon,couleur)
	
	for pr in all(prouts) do
		circ(pr.x,pr.y,pr.timer/3,
							4+pr.timer%3)
	end
end

