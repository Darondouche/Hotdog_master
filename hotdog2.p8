--score

function draw_score()
	palt(0,false)
	palt(12,true)
	spr(48,camx+2,camy+2)
	print_score_outline("X"
	..p.hotdogs,camx+2+8,camy+2+2)
end

function print_score_outline(text,x,y)
	print(text,x-1,y,0)	
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)	
end
